<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG PUSCH_DELICATED" macro_code="3000" macro_name="Change PUSCH Configuration Delicated"
            command_group="7" para_num="6" must_give="1" maindisplay="1 2 3 4 5 6">

        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>

        <para index='2' name='beta_offset_ack_index' description='Beta Offset ACK Index' type='number'>
            <range min='0' max='15'/>
        </para>
        <para index='3' name='beta_offset_ri_index' description='Beta Offset RI Index' type='number'>
            <range min='0' max='15'/>
        </para>
        <para index='4' name='beta_offset_cqi_index' description='Beta Offset CQI Index' type='number'>
            <range min='0' max='15'/>
        </para>

        <para index='5' name='srs_duration' description='SRS Duration' type='list'>

            <list name="Single" value="0"/>
            <list name="Periodic" value="1"/>

        </para>

        <para index='6' name='srs_hopping_enable' description='SRS Hopping Enable' type='list'>

            <list name="Disable" value="0"/>
            <list name="Enable" value="1"/>

        </para>

    </command>
</macro>
