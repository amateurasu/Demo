<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG DEFAULT_PATH_LOSS" macro_code="3001" macro_name="Change Default Path Loss Target SINR Map"
            command_group="7" para_num="2" maindisplay="1 2" must_give="1 2">

        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>

        <para index="2" name="default_path_loss" description="Default Pathloss" type='number'>
            <range min="0" max="200"/>
        </para>

    </command>
</macro>
