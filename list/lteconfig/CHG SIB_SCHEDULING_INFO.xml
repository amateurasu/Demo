<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG SIB_SCHEDULING_INFO" macro_code="lamnt13" macro_name="Change SIB scheduling Information"
            command_group="7" para_num="3" maindisplay="1 2 3" must_give="1">
        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>
        <para index="2" name="SI_PERIODICITY" description="SI periodicity" type="number">
            <range min="0" max="6"/>
        </para>
        <para index="3" name="SIB_TYPE" description="SIB type" type="number">
            <range min="0" max="15"/>
        </para>

    </command>
</macro>
