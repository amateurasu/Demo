        </para>
        <para index='13' name='DLQLoadUPThresh' description='DLQLoadUPThresh' type='number'>
            <range min='0' max='255'/>
        </para>
        <para index='14' name='DLQLoadLowThresh' description='DLQLoadLowThresh' type='number'>
