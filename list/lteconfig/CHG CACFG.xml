            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>
        <para index="2" name="isCAEligible" description="CA supported?" multi_value="1" type="list">
            <list name="No" value="0" description="No"/>
            <list name="Yes" value="1" description="Yes"/>
        </para>
        <para index="3" name="minNumUEThruputReport" description="Minimum UE thruput report" type="number">
            <range min='1' max='20'/>
        </para>
        <para index='4' name='applicableBitrateDL' description='Applicable bitrate DL' type='number'>
            <range min='5' max='18446744073709551615'/>
        </para>
        <para index='5' name='isMASA' description='isMASA' multi_value='1' type='list'>
            <list name="No" value="0" description="No"/>
