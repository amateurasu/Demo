        </para>
        <para index='6' name='ScellDeactTimer' description='Scell deactive timer' multi_value='1' type='list'>
            <list name="FR2" value="RF2" description="RF2"/>
            <list name='RF4' value='RF4' description='RF4'/>
            <list name='RF8' value='RF8' description='RF8'/>
            <list name='RF16' value='RF16' description='RF16'/>
            <list name='RF32' value='RF32' description='RF32'/>
            <list name='RF64' value='RF64' description='RF64'/>
            <list name='RF128' value='RF128' description='RF128'/>
            <list name='RFinf' value='RFinf' description='RFinf'/>
