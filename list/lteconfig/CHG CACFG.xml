        <para index='7' name='loadCalcWindowSize' description='Load calculator Window size' type='number'>
            <range min='100' max='5000'/>
        </para>
        <para index='8' name='loadDisparityThresh' description='Load Disparity Threshold' type='number'>
            <range min='0' max='100'/>
        </para>
        <para index='9' name='MCSDiffThresh' description='MSC Different Threshold' type='number'>
