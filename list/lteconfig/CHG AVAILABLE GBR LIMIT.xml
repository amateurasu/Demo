<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG AVAILABLE_GBR_LIMIT" macro_code="3000" macro_name="Change Available GBR Limit" command_group="7"
            para_num="3" must_give="1" maindisplay="1 2 3">
        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>

        <para index='2' name='dl_gbr_limit' description='DL GBR Limit' type='number' default='500'>
            <range min='1' max='20000'/>
        </para>
        <para index='3' name='ul_gbr_limit' description='UL GBR Limit' type='number' default='500'>
            <range min='1' max='20000'/>
        </para>
    </command>
</macro>
