<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG RAN_EMER_SERV" macro_code="3000" macro_name="Change LTE RAN Emergency Service" command_group="8"
            para_num="3" must_give="1" maindisplay="1 2 3">
        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0"/>
            <list name="1" value="1"/>
            <list name="2" value="2"/>
        </para>

        <para index="2" name="IMS_EMERGENCY_SUPPORT_R9" description="IMS Emergency Support R9" type="list">
            <list name="TRUE" value="0"/>
            <list name="OTHER" value="1"/>
        </para>

        <para index="3" name="EMERGENCY_ERAB_ARP" description="Emergency ERAB ARP" type="number">
            <range min="0" max="255"/>
        </para>
    </command>
</macro>
