<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG SCHEDULER_PARAM" macro_code="3000" macro_name="Change Scheduler Parameters" command_group="7"
            para_num="4" must_give="1" maindisplay="1 2 3 4">
        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>
        <para index='2' name='maxMcs16QAM' description='Max MCS 16 QAM' type='number'>
            <range min='0' max='28'/>
        </para>
        <para index='3' name='dlSchedulerStrategy' description='DL Scheduler Strategy' type='list'>
            <list name='PFS' value='0'/>
            <list name='RR' value='1'/>
            <list name='QoS Scheduler' value='2'/>
        </para>
        <para index='4' name='ulSchedulerStrategy' description='UL Scheduler Strategy' type='list'>
            <list name='PFS' value='0'/>
            <list name='RR' value='1'/>
            <list name='QoS Scheduler' value='2'/>
        </para>
    </command>
</macro>
