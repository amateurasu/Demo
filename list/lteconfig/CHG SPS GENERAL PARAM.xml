<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG SPS GENERAL PARAM" macro_code="3000" macro_name="Change SPS General Param" command_group="8"
            para_num="3" must_give="" maindisplay="1 2 3">
        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>

        <para index='2' name='max_sps_ues' description='Max SPS UEs' type='number' default='20'>
            <range min='1' max='255'/>
        </para>

        <para index='3' name='maxSpsDciAttempt' description='Max SPS DCI Attempt' type='number' default='4'>
            <range min='1' max='8'/>
        </para>
    </command>
</macro>
