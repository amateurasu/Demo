<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG PATH_LOSS_SINR_MAP" macro_code="3001" macro_name="Change Alpha Pathloss Target SINR Map"
            command_group="7" para_num="2" maindisplay="1 2 3 4 5" must_give="1 2">

        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>

        <para index="2" name="index" description="index" type="list" multi-value="1" parentAttr='1'>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
            <list name="3" value="3" description="3"/>
            <list name="4" value="4" description="4"/>
            <list name="5" value="5" description="5"/>
            <list name="6" value="6" description="6"/>
        </para>

        <para index="3" name="start_PL" description="Start Pathloss" type='number'>
            <range min="0" max="200"/>
        </para>

        <para index="4" name="end_PL" description="End Pathloss" type='number'>
            <range min="0" max="200"/>
        </para>

        <para index="5" name="target_SINR" description="Target SINR" type='number'>
            <range min="0" max="200"/>
        </para>

    </command>
</macro>
