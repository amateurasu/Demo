<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG IDLEMODE_UTRA" macro_code="3000" macro_name="Change Idle Mode IRAT UTRA Parameters"
            command_group="7" para_num="2" maindisplay="1 2 3 4" must_give="1">

        <para index="1" name="CellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>
        <para index="2" name="TReselectionUTRA" description="TReselectionUTRA" type="number">
            <range min="0" max="7"/>
        </para>

        <para index="3" name="TReselectionUTRASFMedium" description="TReselectionUTRASFMedium" type="list">
            <list name="0.25" value="25"/>
            <list name="0.5" value="50"/>
            <list name="0.75" value="75"/>
            <list name="1.0" value="100"/>
        </para>
        <para index="4" name="TReselectionUTRASFHigh" description="TReselectionUTRASFHigh" type="list">
            <list name="0.25" value="25"/>
            <list name="0.5" value="50"/>
            <list name="0.75" value="75"/>
            <list name="1.0" value="100"/>
        </para>

    </command>
</macro>
