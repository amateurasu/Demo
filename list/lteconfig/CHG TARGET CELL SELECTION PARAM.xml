<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG TARGET_CELL_SELECTION_PARAM" macro_code="3000"
            macro_name="Change Target Cell Selection Parameters" command_group="7" para_num="6" must_give="1"
            maindisplay="1 2 3 4 5 6">
        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>

        <para index='2' name='daho_cell_based_ho_enable' description='DAHO Cell Base Handover' type='list'>
            <list name='Disable' value='0'/>
            <list name='Enable' value='1'/>
        </para>
        <para index='3' name='ue_measurement_based_ho_enable' description='UE Measurement Based Handover' type='list'
                default='Enable'>
            <list name='Disable' value='0'/>
            <list name='Enable' value='1'/>
        </para>
        <para index='4' name='spid_based_ho_enable' description='SPID Based Handover' type='list'>
            <list name='Disable' value='0'/>
            <list name='Enable' value='1'/>
        </para>
        <para index='5' name='ue_history_based_ho_enable' description='UE History Based Handover' type='list'>
            <list name='Disable' value='0'/>
            <list name='Enable' value='1'/>
        </para>
        <para index='6' name='neighboring_cell_load_based_ho_enable' description='Neighboring Cell Load Based Handover'
                type='list'>
            <list name='Disable' value='0'/>
            <list name='Enable' value='1'/>
        </para>
    </command>
</macro>
