<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG ADDITIONAL_PACKET_SCHEDULING_PARAMS" macro_code="3000"
            macro_name="Change Additional Packet Scheduling Params" command_group="7" para_num="5"
            maindisplay="1 2 3 4 5" must_give="1">
        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>
        <para index="2" name="frequency_selective_scheduling" description="Frequency Selective Scheduling" type="list">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
            <list name="3" value="3" description="3"/>
        </para>
        <para index="3" name="cqi_reporting_mode" description="Cqi Reporting Mode" type="list">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
            <list name="3" value="3" description="3"/>
        </para>

        <para index="4" name="dl_mcs" description="Downlink MCS" type="number">
            <range min="0" max="28"/>
        </para>
        <para index="5" name="ul_mcs" description="Uplink MCS" type="number">
            <range min="0" max="28"/>
        </para>

    </command>
</macro>
