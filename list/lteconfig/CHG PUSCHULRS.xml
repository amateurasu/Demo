<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG PUSCHULRS" macro_code="3000" macro_name="Change LTE RAN PHY PUSCHULRS" command_group="7"
            para_num="5" maindisplay="1 2 3 4 5" must_give="1">
        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>
        <para index="2" name="GroupHoppingEnabled" description="Group Hopping Enabled" type="list">
            <list name="Yes" value="1"/>
            <list name="No" value="0"/>
        </para>
        <para index="3" name="SequenceHoppingEnabled" description="Sequence Hopping Enabled" type="list">
            <list name="Yes" value="1"/>
            <list name="No" value="0"/>
        </para>

        <para index="4" name="GroupAssignmentPUSCH" description="Group Assignment PUSCH" type="number">
            <range min="0" max="29"/>
        </para>

        <para index="5" name="CyclicShift" description="PUSCH Cyclic Shift" type="number">
            <range min="0" max="7"/>

        </para>

    </command>
</macro>
