<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG DIMENSION_PARAM" macro_code="3000" macro_name="Change Dimension Parameters" command_group="7"
            para_num="2" must_give="1" maindisplay="1 2 3">
        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>
        <para index='2' name='Max_no_of_mmes' description='The maximum number of MMEs supported' type='number'>
            <range min='0' max='16'/>
        </para>

        <para index='3' name='NumPeerRec' description='Number of Peers Entities e-GTPU Supported' type='number'>
            <range min='0' max='65535'/>
        </para>

    </command>
</macro>
