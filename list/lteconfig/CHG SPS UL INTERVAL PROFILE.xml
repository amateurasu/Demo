<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG SPS UL INTERVAL PROFILE" macro_code="3000" macro_name="Change SPS UL Interval Profile"
            command_group="7" para_num="5" must_give="" maindisplay="1 2 3 4 5">
        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1">
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>

        <para index='2' name='pid' description='Profile ID' type='number' parentAttr='1'>
            <range min='1' max='16'/>
        </para>

        <para index='3' name='ulMinMcsSps' description='UL Min MCS SPS' type='number' default='6'>
            <range min='0' max='15'/>
        </para>
        <para index='4' name='ulMcsMargin' description='UL MCS Margin' type='number' default='4'>
            <range min='0' max='8'/>
        </para>
        <para index='5' name='ulTbSizeMarginSps' description='UL Tb Size Margin SPS' type='number' default='4'>
            <range min='0' max='20'/>
        </para>
    </command>
</macro>
