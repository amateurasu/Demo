<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG CORE" macro_code="3000" macro_name="Configure Core Dump Level" command_group="7" para_num="3"
            maindisplay="1 2" must_give="1 2">
        <para index="1" name="level" description="Core Dump Level" type="list">
            <list name="no core" value="0" description="disable"/>
            <list name="have core" value="1" description="enable"/>
        </para>
        <para index="2" name="cardType" description="Card Type" type="list">
            <list name="CTC" value="1" description="CTC"/>
            <list name="BBC" value="2" description="BBC"/>
            <child_select>
                <value min="2" max="2"/>
                <open index="3" must_give=""/>
            </child_select>
            <list name="ALL" value="4" description="ALL"/>
        </para>
        <para index="3" name="BbcNum" description="BBC Number" type="list">
            <list name="BBC1" value="1" description="BBC1"/>
            <list name="BBC2" value="2" description="BBC2"/>
            <list name="BBC3" value="3" description="BBC3"/>
            <list name="ALL" value="4" description="ALL"/>
        </para>
    </command>
</macro>
