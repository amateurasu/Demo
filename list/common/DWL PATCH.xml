<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="DWL PATCH" macro_code="3000" macro_name="Download Patch" command_group="7" para_num="5"
            maindisplay="1 2 3 4 5" must_give="1 2 3 4 5">
        <para index="1" name="IpMode" description="IP Mode" multi_value="1" type="list">
            <list name="IPV4" value="IPV4" description="IPV4"/>
            <!--<list name="IPV6" value="IPV6" description="IPV6"/>-->
        </para>
        <para index="2" name="ServerIP" description="SFTP Server IP" type="ipaddress"
                pattern="/^((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$/">
        </para>
        <para index="3" name="UserName" description="User Name" type="text" minlength="1" maxlength="31">
        </para>
        <para index="4" name="password" description="Password" type="password" minlength="1" maxlength="31">
        </para>
        <para index="5" name="Path" description="File Path" type="text" minlength="1" maxlength="127">
        </para>

    </command>
</macro>
