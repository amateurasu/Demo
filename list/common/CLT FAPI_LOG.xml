<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CLT FAPI_LOG" macro_code="3000" macro_name="Collect Current FAPI Log" command_group="7" para_num="5"
            maindisplay="1 2 3" must_give="1 2 3">

        <para index="1" name="Desc" description="Description" type="text" minlength="1" maxlength="64">
        </para>

        <para index="2" name="BbcNum" description="BBC Number" type="list">
            <list name="BBC1" value="1" description="BBC1"/>
            <list name="BBC2" value="2" description="BBC2"/>
            <list name="BBC3" value="3" description="BBC3"/>

        </para>

        <para index="3" name="TimeCollect" description="Time Collect (s)" type="number">
            <range min='1' max='300'/>
        </para>

    </command>
</macro>
