<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="START RADIO_TRACE" macro_code="3000" macro_name="Start Radio Trace" command_group="8" para_num="1"
            maindisplay="1 2 3 4" must_give="1 2 3 4">
        <para index="1" name="cellId" parentAttr='1' description="Cell ID" type="list" multi-value="1" default='0'>
            <list name="0" value="0" description="0"/>
            <list name="1" value="1" description="1"/>
            <list name="2" value="2" description="2"/>
        </para>
        <para index="2" name="sampleRate" description="Sample Rate" type="number">
            <range min='0' max='255'/>
        </para>
        <para index="3" name="startTime" description="Trace Start Time" type="datetime">
        </para>
        <para index="4" name="endTime" description="Trace End Time" type="datetime">
        </para>
    </command>
</macro>
