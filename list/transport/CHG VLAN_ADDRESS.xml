<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG VLAN_ADDRESS" macro_code="3000" macro_name="Change VLAN Address" command_group="7" para_num="3"
            maindisplay="1 2 3" must_give="1 2 3">

        <para index="1" name="vlanName" description="Vlan Name" type="text" minlength="1" maxlength="63">
        </para>

        <para index="2" name="vlanIp" description="Vlan IP Addess" type="ipaddress"
                pattern="/^((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$/">
        </para>

        <para index="3" name="vlanNetmask" description="Vlan IP Netmask" type="ipaddress"
                pattern="/^((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$/">
        </para>

    </command>
</macro>
