<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG FEATURE_STATE" macro_code="3000" macro_name="Change Feature State" command_group="7" para_num="4"
            maindisplay="1 2 3" must_give="1 2 3">

        <para index="1" name="FeatureName" description="Feature Name" type="text" minlength="1" maxlength="256">
        </para>
        <para index="2" name="FeatureLevel" description="Feature Level" type="list">
            <list name="NE" value="0"/>
            <list name="Cell" value="1"/>

            <child_select>
                <value min="1" max="1"/>
                <open index="4" must_give="4"/>
            </child_select>

        </para>

        <para index="3" name="FeatureState" description="Feature State" type="list">
            <list name="Disable" value="0"/>
            <list name="Enable" value="1"/>

        </para>

        <para index="4" name="CellId" description="Cell Id" type="list">
            <list name="0" value="0"/>
            <list name="1" value="1"/>
            <list name="2" value="2"/>
            <list name="All" value="255"/>

        </para>

    </command>
</macro>
