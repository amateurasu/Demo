<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG NE" macro_code="3001" macro_name="Change NE Information" command_group="7" para_num="9"
            maindisplay="1 2 3 4 5 6 7 8"
            must_give="1 2" must_be_confirm="Are you sure you want to active a new software version?">

        <para index="1" name="NeId" description="NE ID" type="number">
            <range min="0" max="1048575"/>
        </para>
        <para index="2" name="NeType" description="NE Type" type="list" default="0">
            <list name="Home" value="1"/>
            <list name="Macro" value="0"/>
        </para>
        <para index="3" name="NeName" description="NE Name" type="text" minlength="1" maxlength="63">
        </para>
        <para index="4" name="Longitude" description="Longitude" type="number">
            <range min="-1800000" max="1800000"/>
        </para>
        <para index="5" name="Latitude" description="Latitude" type="number">
            <range min="-900000" max="900000"/>
        </para>
        <para index="6" name="SiteId" description="Site ID" type="number">
            <range min="0" max="1048575"/>
        </para>
        <para index="7" name="SiteName" description="Site Name" type="text" minlength="1" maxlength="63">
        </para>
        <para index="8" name="VendorName" description="Vendor Name" type="text" minlength="1" maxlength="63">
        </para>
    </command>
</macro>
