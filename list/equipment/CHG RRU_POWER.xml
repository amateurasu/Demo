<?xml version="1.0" encoding="UTF-8"?>
<macro>
    <command name="CHG RRU_POWER" macro_code="3000" macro_name="Change RRU POWER" command_group="7" para_num="6"
            maindisplay="1 2 3 4 5" must_give="1 2 3 4 5 ">

        <para index="1" name="RackNumber" description="Connected BBC Rack Number" type="list" default="0">
            <list name='0' value='0'/>
            <list name='1' value='1'/>
            <list name='2' value='2'/>
            <list name='3' value='3'/>
        </para>
        <para index="2" name="SubRackNumber" description="Connected BBC Subrack Number" type="list" default="0">
            <list name='0' value='0'/>
            <list name='1' value='1'/>
            <list name='2' value='2'/>
            <list name='3' value='3'/>
        </para>
        <para index="3" name="SlotNumber" description="BBC" type="list" default="2">
            <list name='BBC1' value='2'/>
            <list name='BBC2' value='3'/>
            <list name='BBC3' value='4'/>
        </para>
        <para index="4" name="PortNumber" description="Connected CPRI Port Number" type="list" default="0">
            <list name='0' value='0'/>
            <list name='1' value='1'/>
        </para>

        <para index='5' name='Power' description='Power' type='list' default='0'>
            <list name='5w' value='5'/>
            <list name='10w' value='10'/>
            <list name='15w' value='15'/>
            <list name='20w' value='20'/>
            <list name='25w' value='25'/>
            <list name='30w' value='30'/>
            <list name='35w' value='35'/>
            <list name='40w' value='40'/>
        </para>
    </command>
</macro>
